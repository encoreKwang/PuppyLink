{
  "name": "puppylink",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "vite --host --port=3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
