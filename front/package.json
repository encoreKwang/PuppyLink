    "match-sorter": "^6.3.1",
    "pretendard": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.7.0",
    "react-toastify": "^9.1.1",
    "recoil": "^0.7.6",
    "sort-by": "^1.2.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
